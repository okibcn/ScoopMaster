    "url": "https://www.google.com/dl/release2/chrome_component/acjtfwguunyux65a2lvmeg5zrtna_8456/hfnkpimlhhgieaddgfemjhofmfblmnib_8456_all_acamonzh5qy6up4brymlsbjmniaq.crx3",
    "hash": "13c12120b3b1231ed17c9174af6571b1d479d27052384c0507d9b08c2f2c2937",
    "installer": {
        "script": [
            "$dest = \"$dir\\CertificateRevocation\"",
            "Expand-7zipArchive \"$dir\\$fname\" $dest -Removal",
