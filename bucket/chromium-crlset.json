    "hash": "35d2b31a1519a9185deba074951fd17d07f2e7f95c6c1bd8d73f0e6c8ee3a49a",
    "installer": {
        "script": [
            "$dest = \"$dir\\CertificateRevocation\"",
            "Expand-7zipArchive \"$dir\\$fname\" $dest -Removal",
