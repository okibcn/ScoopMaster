    "hash": "1a6960cc9db9d263db659529ca97509b950bd2b234127ebfc31da0ef8c1260ad",
    "installer": {
        "script": [
            "$dest = \"$dir\\CertificateRevocation\"",
            "Expand-7zipArchive \"$dir\\$fname\" $dest -Removal",
