    "hash": "3ecce3cf6229a8fd6cac97eebb7b7532f7061c018e972dc6bf277e12568e1ec4",
    "installer": {
        "script": [
            "$dest = \"$dir\\CertificateRevocation\"",
            "Expand-7zipArchive \"$dir\\$fname\" $dest -Removal",
