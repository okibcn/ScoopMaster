    "url": "https://www.google.com/dl/release2/chrome_component/nfeooijf7ria2nyc3b6ywnj3ca_8035/hfnkpimlhhgieaddgfemjhofmfblmnib_8035_all_acejzn7aymouexhouczzwxw7qseq.crx3",
    "hash": "1b7329b4d9ece7775214c20bc116e0fc7ef7e1a4d1d3887c9a17bb591cdb5054",
    "installer": {
        "script": [
            "$dest = \"$dir\\CertificateRevocation\"",
            "Expand-7zipArchive \"$dir\\$fname\" $dest -Removal",
