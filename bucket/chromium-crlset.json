    "hash": "d4175f0f7067bca2f276f9cabadff02c14ec08f168f5a190eae5adaad23be4f9",
    "installer": {
        "script": [
            "$dest = \"$dir\\CertificateRevocation\"",
            "Expand-7zipArchive \"$dir\\$fname\" $dest -Removal",
