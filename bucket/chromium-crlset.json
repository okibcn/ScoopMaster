    "hash": "dd9c8f9695bd9ec5c9288a3c7ba4ba13033c04d0ed860c7c01d79f48354acd3a",
    "installer": {
        "script": [
            "$dest = \"$dir\\CertificateRevocation\"",
            "Expand-7zipArchive \"$dir\\$fname\" $dest -Removal",
