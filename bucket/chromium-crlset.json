    "hash": "caf6107592a54bd0bb55e89c14d7d21c8ab667b47f9b8c92fd12d8d152e5ddcc",
    "installer": {
        "script": [
            "$dest = \"$dir\\CertificateRevocation\"",
            "Expand-7zipArchive \"$dir\\$fname\" $dest -Removal",
