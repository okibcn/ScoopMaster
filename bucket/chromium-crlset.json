    "url": "https://www.google.com/dl/release2/chrome_component/adi4l475jzmrfekbukqg3g52il3q_8355/hfnkpimlhhgieaddgfemjhofmfblmnib_8355_all_aclsl2nss2r2shq4br2ggy57rh2a.crx3",
    "hash": "da59dccf6eeb15bd6164367a236ba8760bd9f3abf9bfa7dcb24c0490827dc3a2",
    "installer": {
        "script": [
            "$dest = \"$dir\\CertificateRevocation\"",
            "Expand-7zipArchive \"$dir\\$fname\" $dest -Removal",
