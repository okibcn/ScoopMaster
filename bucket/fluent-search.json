    "post_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "persist -dir @(\"$dir\\Blast\") @(\"$persist_dir\\Blast\")",
        "create_app_lnk \"$dir\\FluentSearch.exe\" \"$desktop\\FluentSearch.lnk\"",
        "create_app_lnk \"$dir\\FluentSearch.exe\" \"$scoop_apps_lnk\\FluentSearch.lnk\""
    ],
    "pre_uninstall": [
        "$bucket=(scoop bucket list | where-Object { $_.Source -match '(gitee|github).com/abgox/abgo_bucket'})[0].Name",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "stop_process",
        "remove_files @(\"$desktop\\FluentSearch.lnk\",\"$scoop_apps_lnk\\FluentSearch.lnk\",\"$env:LocalAppData\\FluentSearch\")"
    ],
    "checkver": {
        "url": "https://v.abgo-bucket.workers.dev/b/BlastApps/FluentSearch",
        "jsonpath": "$.version[0]"
    },
    "autoupdate": {
        "url": "https://stfluentsearch.blob.core.windows.net/fluent-search/fluent-search-portable.zip"
    }
