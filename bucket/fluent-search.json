{
    "version": "0.9.92.998",
    "description": "使用它，你可以搜索正在运行的应用程序、浏览器标签、应用程序内内容、文件等。With it, you can search for running apps, browser tabs, in-app content, files and more.",
    "homepage": "https://fluentsearch.net",
    "license": "Unknown",
    "url": "https://stfluentsearch.blob.core.windows.net/fluent-search/Fluent-Search.exe#/_installer_.exe",
    "hash": "a23bea3776b443a38737ae7d9500a0ea6049ddfb71ad6eaaf9bf6fe53c93b568",
    "innosetup": true,
    "shortcuts": [
        [
            "FluentSearch.exe",
            "FluentSearch"
        ]
    ],
    "pre_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "persist -dir @(\"$env:AppData\\Blast\") @(\"$persist_dir\\Blast\")",
        "create_app_lnk \"$dir\\FluentSearch.exe\" \"$desktop\\FluentSearch.lnk\""
    ],
    "pre_uninstall": [
        "$bucket=(scoop bucket list | where-Object { $_.Source -match '(gitee|github).com/abgox/abgo_bucket'})[0].Name",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "stop_process",
        "remove_files @(\"$desktop\\FluentSearch.lnk\",\"$env:AppData\\Blast\",\"$env:LocalAppData\\FluentSearch\")"
    ],
    "checkver": {
        "script": [
            "$bucket=(scoop bucket list | where-Object { $_.Source -match '(gitee|github).com/abgox/abgo_bucket'})[0].Name",
            ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
            "$dl_info = get_installer_info 'BlastApps/FluentSearch'",
            "\"ver:$($dl_info.PackageVersion);\"",
            "\"x64:$($dl_info.x64_exe.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>.+?); x64:(?<x64>.+?);"
    },
    "autoupdate": {
        "url": "$matchX64#/_installer_.exe",
        "hash": {
            "url": "https://raw.githubusercontent.com/microsoft/winget-pkgs/master/manifests/b/BlastApps/FluentSearch/$version/BlastApps.FluentSearch.installer.yaml",
            "regex": "Architecture: x64[\\s\\S]+?InstallerSha256:\\s?(.+)"
        }
    }
}
