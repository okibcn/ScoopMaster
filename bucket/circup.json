{
    "version": "1.5.1",
    "description": "CircuitPython library updater. ",
    "homepage": "https://github.com/adafruit/circup",
    "license": "MIT",
    "url": "https://github.com/adafruit/circup/archive/refs/tags/1.5.1.tar.gz",
    "hash": "6b87a10d10daedc89a10edce182c3e3c899cc3b11fb5e9f1a2c91cd766db45a0",
    "depends": "python",
    "installer": {
        "script": [
            "Push-Location \"$dir\"",
            "python -m venv circup",
            ".\\circup\\Scripts\\activate",
            "try {",
            "    python -m pip install wheel",
            "    python -m pip install circup",
            "}",
            "finally {",
            "    Remove-Item \"circup-$version\" -Recurse",
            "    deactivate",
            "    Pop-Location",
            "}"
        ]
    },
    "bin": "circup\\Scripts\\circup.exe",
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/adafruit/circup/archive/refs/tags/$version.tar.gz"
    }
}
