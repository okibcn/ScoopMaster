    "pre_install": [
    "# Create initial config files",
    "if (!(Test-Path -Path \"$dir\\MusicTag.db\")) {",
    "    (New-Item -ItemType \"file\" -Path \"$dir\" -Name \"MusicTag.db\") | Out-Null",
    "}",
    "# Create initial config files",
    "if (!(Test-Path -Path \"$dir\\MusicTag.dat\")) {",
