{
    "version": "0.0.7-alpha",
    "description": "Patched for Warp to work in iran.",
    "homepage": "https://github.com/uoosef/wireguard-go",
    "license": "MIT",
    "notes": "",
    "architecture": {
        "64bit": {
            "url": "https://github.com/uoosef/wireguard-go/releases/download/v0.0.7-alpha/warp-windows-amd64.7242f5.zip",
            "hash": "f6bfd58f80af7dfbcbf9d004cac5194e515c9b53a80b8714261b3413b5adbe3e"
        }
    },
    "checkver": {
        "url": "https://api.github.com/repos/uoosef/wireguard-go/releases",
        "jsonpath": "$.[0].assets[0].browser_download_url",
        "regex": "v(?<version>[\\d.]+-alpha)/[^/]+\\.(?<commit>[\\w]+)\\.zip$"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/uoosef/wireguard-go/releases/download/v$version/warp-windows-amd64.$matchCommit.zip"
            }
        }
    },
    "pre_install": [
        "'wgcf-profile.ini', 'wgcf-identity.json' | ForEach-Object {",
        "    if (!(Test-Path \"$persist_dir\\$_\")) { New-Item \"$dir\\$_\" -ItemType File | Out-Null }",
        "};"
    ],
    "persist": [
        "wgcf-profile.ini",
        "wgcf-identity.json"
    ],
    "bin": "warp.exe",
    "shortcuts": [
        [
            "warp.exe",
            "warp",
            "wireguard-go"
        ]
    ]
}
