{
    "version": "0.12.2",
    "description": "Backrest is a web UI and orchestrator for restic backup.",
    "homepage": "https://github.com/garethgeorge/backrest",
    "license": "GPL-3.0-or-later",
    "depends": "restic",
    "architecture": {
        "64bit": {
            "url": "https://github.com/garethgeorge/backrest/releases/download/v0.12.2/backrest_Windows_x86_64.zip",
            "hash": "1b4243f195cad840b7be6deaf86fea3e8923e2a2f006553733d4dbc0ae42d196"
        },
        "arm64": {
            "url": "https://github.com/garethgeorge/backrest/releases/download/v0.12.2/backrest_Windows_arm64.zip",
            "hash": "f33c2aaa8a041866211342fabba80c92b24eb10bfe7fb06c6bb0aa5a3e85240e"
        }
    },
    "bin": "backrest.exe",
    "env_set": {
        "BACKREST_CONFIG": "$dir\\backrest\\config.json",
        "BACKREST_DATA": "$dir\\backrest\\data",
        "BACKREST_RESTIC_COMMAND": "$(appdir restic $global)\\current\\restic.exe"
    },
    "persist": "backrest",
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/garethgeorge/backrest/releases/download/v$version/backrest_Windows_x86_64.zip"
            },
            "arm64": {
                "url": "https://github.com/garethgeorge/backrest/releases/download/v$version/backrest_Windows_arm64.zip"
            }
        },
        "hash": {
            "url": "$baseurl/backrest_$version_checksums.txt"
        }
    }
}
