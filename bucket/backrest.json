{
    "version": "0.11.0",
    "description": "Backrest is a web UI and orchestrator for restic backup.",
    "homepage": "https://github.com/garethgeorge/backrest",
    "license": "GPL-3.0-or-later",
    "depends": "restic",
    "architecture": {
        "64bit": {
            "url": "https://github.com/garethgeorge/backrest/releases/download/v0.11.0/backrest_Windows_x86_64.zip",
            "hash": "8ed306e772195f4fc94ffc9ff894877d140d787a62e91ca6cd3116ec9fba04f6"
        },
        "arm64": {
            "url": "https://github.com/garethgeorge/backrest/releases/download/v0.11.0/backrest_Windows_arm64.zip",
            "hash": "c058e1821cd20208a0a6b58c936a1b7f6c7931c74125bb26adebb0726dc807d0"
        }
    },
    "bin": "backrest.exe",
    "env_set": {
        "BACKREST_CONFIG": "$dir\\backrest\\config.json",
        "BACKREST_DATA": "$dir\\backrest\\data",
        "BACKREST_RESTIC_COMMAND": "$(appdir restic $global)\\current\\restic.exe"
    },
    "persist": "backrest",
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/garethgeorge/backrest/releases/download/v$version/backrest_Windows_x86_64.zip"
            },
            "arm64": {
                "url": "https://github.com/garethgeorge/backrest/releases/download/v$version/backrest_Windows_arm64.zip"
            }
        },
        "hash": {
            "url": "$baseurl/backrest_$version_checksums.txt"
        }
    }
}
