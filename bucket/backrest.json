{
    "version": "0.9.3",
    "description": "Backrest is a web UI and orchestrator for restic backup.",
    "homepage": "https://github.com/garethgeorge/backrest",
    "license": "GPL-3.0-or-later",
    "depends": "restic",
    "architecture": {
        "64bit": {
            "url": "https://github.com/garethgeorge/backrest/releases/download/v0.9.3/backrest_Windows_x86_64.zip",
            "hash": "806b0ad6d53ca13ca859d52d09daf19b2265c7ee09a3405242a44da1da6b1917"
        },
        "arm64": {
            "url": "https://github.com/garethgeorge/backrest/releases/download/v0.9.3/backrest_Windows_arm64.zip",
            "hash": "4c4304e269e9947bde6e7dce2b98da93ab6ec358c75f9c6a964e9a3b8b7a32e2"
        }
    },
    "bin": "backrest.exe",
    "env_set": {
        "BACKREST_CONFIG": "$dir\\backrest\\config.json",
