{
    "version": "0.13.0",
    "description": "Backrest is a web UI and orchestrator for restic backup.",
    "homepage": "https://github.com/garethgeorge/backrest",
    "license": "GPL-3.0-or-later",
    "depends": "restic",
    "architecture": {
        "64bit": {
            "url": "https://github.com/garethgeorge/backrest/releases/download/v0.13.0/backrest_Windows_x86_64.zip",
            "hash": "4de6c0148a1e8464e727f9598659b000d3fb05db9ea9947ec20ba6e373894eca"
        },
        "arm64": {
            "url": "https://github.com/garethgeorge/backrest/releases/download/v0.13.0/backrest_Windows_arm64.zip",
            "hash": "4e05472e451fe0b50bcc74443e7ce60f3a6ae33bd55f24d22cec520d79120d86"
        }
    },
    "bin": "backrest.exe",
    "env_set": {
        "BACKREST_CONFIG": "$dir\\backrest\\config.json",
        "BACKREST_DATA": "$dir\\backrest\\data",
        "BACKREST_RESTIC_COMMAND": "$(appdir restic $global)\\current\\restic.exe"
    },
    "persist": "backrest",
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/garethgeorge/backrest/releases/download/v$version/backrest_Windows_x86_64.zip"
            },
            "arm64": {
                "url": "https://github.com/garethgeorge/backrest/releases/download/v$version/backrest_Windows_arm64.zip"
            }
        },
        "hash": {
            "url": "$baseurl/backrest_$version_checksums.txt"
        }
    }
}
