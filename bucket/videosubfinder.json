{
    "version": "5.60",
    "description": "Extracts hardcoded text (hardsub) from video",
    "homepage": "https://sourceforge.net/projects/videosubfinder/",
    "license": "Public Domain",
    "suggest": {
        "vcredist": [
            "extras/vcredist2019",
            "extras/vcredist-aio"
        ]
    },
    "architecture": {
        "64bit": {
            "url": "https://downloads.sourceforge.net/project/videosubfinder/VideoSubFinder_5.60_x64.zip",
            "hash": "sha1:b7112a6dcc100c2f0cc799077436ecbbe8892ebd",
            "extract_dir": "Release_x64"
        },
        "32bit": {
            "url": "https://downloads.sourceforge.net/project/videosubfinder/VideoSubFinder_5.60_x86.zip",
            "hash": "sha1:d7e4343e8f3acaa9a84ecdeb8d8be9abd4215e77",
            "extract_dir": "Release"
        }
    },
    "persist": [
        "settings\\general.cfg",
        "TXTImages",
        "TXTImagesJoined",
        "TXTResults"
    ],
    "bin": "VideoSubFinderWXW.exe",
    "shortcuts": [
        [
            "VideoSubFinderWXW.exe",
            "VideoSubFinder"
        ]
    ],
    "checkver": {
        "sourceforge": "videosubfinder/",
        "regex": "VideoSubFinder_([\\d.+]+)_x64.zip"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.sourceforge.net/project/videosubfinder/VideoSubFinder_$version_x64.zip"
            },
            "32bit": {
                "url": "https://downloads.sourceforge.net/project/videosubfinder/VideoSubFinder_$version_x86.zip"
            }
        }
    }
}
